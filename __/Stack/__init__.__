class Stack
  def reverse (?)
    variable self, l = _NARGS - 1, i, s, args = {};

    ifnot (l)
      {
      l = _stkdepth - 1;
      self = ();
      }

    _for i (1, l)
      {
      s = ();
      list_append (args, s);
      }

    ifnot (1 == _NARGS)
      self = ();

    __push_list  (args);
  end
end
