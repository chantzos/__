class String
  def decode (str)
    variable
      d,
      i = 0,
      l = {};

    forever
      {
      (i, d) = strskipchar (str, i);
      if (d)
        list_append (l, d);
      else
        break;
      }

    length (l) ? list_to_array (l) : ['\n'];
  end

  def to_int (str)
    variable retval = atoi (str);

    ifnot (retval)
      if (str != "0")
        return NULL;

    retval;
  end
end
