class Xclnt
  import xclient

  let XDIR = Env->TMP_PATH + "/X";
  let WRFIFO = XDIR + "/Svfifo";
  let RDFIFO = XDIR + "/Clfifo";

  subclass Desk
    __init__
      change_by
        id
        name
    end

    let CUR_ID = 0;
    let TOTAL = 13;

    def change_by_id (did)
      if (0 == (-1 < did < TOTAL) || did == CUR_ID)
        return;
    end

    def change_by_name (did)
      if (0 == (-1 < did < TOTAL) || did == CUR_ID)
        return;
    end
  end
end
